{
  "name": "Dropping Apple by airscape",
  "description" : "Dropping Apple by airscape to never miss an important apple dropping event such as taking a nap",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "/dist/index.html",
    "default_icon": "dlogo.png"
  },
  "permissions": [ "alarms", "notifications", "storage" ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens /dist/index.html"
    }
  }
}
